									<option value="M-Tech">M-Tech</option>
									<option value="MBA">MBA</option>
									<option value="BBA">BBA</option>
									
								</select>
								  
							  </div>
						  </div>

						  <div class="form-group">
							<div class="col-sm-12">
								<input class="form-control" id="Course" value="MCA" placeholder="MCA" name="course" type="text" readonly>
							</div>
						</div>
						 



