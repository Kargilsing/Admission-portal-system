				  <div class="tab-pane active" id="Registration">
					  <form role="form" class="form-horizontal" action="/course_insert" method="post">
						  <div class="form-group">
							  <div class="col-sm-12">
								  <input class="form-control" placeholder="Name" type="text" name="name">
							  </div>
						  </div>
