<%- include('../header.ejs')%>
<br>

<div class=" container-fluid">
    <div class="conatiner">
        <div class="row">
            <div class="col-md-2"></div>
            <div class="col-md-8">
           

       
           
            <div class="table-responsive">
                <% if(message.length > 0){ %>
                    <div class="alert alert-success">
                      <%= message %>
                     </div>
                  <% } %>


           
                <table class="table table-bordered">
                    <thead>
                    <tr>
                        <th>Sr.no</th>
                        <th>Name</th>
                        <th>Email</th>
                        
                        <th>Mobile.No</th>
                       
                      
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% d.forEach((item)=>{ %>
                        <tr>
                            <td><%= item._id %></td>
                            <td><%= item.name %></td>
                            <td><%= item.email %></td>
                          
                            <td><%= item.mobile_no %></td>
                            
                            <td>
                                <a href="/course_view/<%= item._id %>" class="btn btn-info">View</a>
                                <a href="/course_edit/<%= item._id %>" class="btn btn-primary">Edit</a>
                                <a href="/course_delete/<%= item._id %>" class="btn btn-danger">Delete</a>
                            </td>
                        </tr>
                    <% }) %>
                   
                </tbody>
                    
                </table>
            </div>
        </div>
            <div class="col-md-2"></div>

        </div>
      
   
</div>
</div>
<%- include('../footer.ejs')%>